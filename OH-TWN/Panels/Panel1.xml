<PropertyList>

  <game-mode type="bool">false</game-mode>

  <panel include="TWN-panel.xml"/>

  <sim>
    <panel>
      <flip-x type="bool">false</flip-x>
    </panel>
    <frame-rate-throttle-hz>30</frame-rate-throttle-hz>
    <instrument-options>
      <omit-knobs type="bool">true</omit-knobs>
      <omit-screws type="bool">true</omit-screws>
    </instrument-options>
  </sim>

  <communication>
    <listen>
      <host>localhost</host>
      <port>34200</port>
      <style>udp</style>
    </listen>

    <protocol include="Panel1Protocol.xml"/>

  </communication>

</PropertyList>
